			<remove invariant="System.Data.SQLite" />
			<add name="SQLite Data Provider" invariant="System.Data.SQLite" description=".NET Framework Data Provider for SQLite" type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite" />
		</DbProviderFactories>
	</system.data>
	<appSettings>
		<add key="CompanyName" value="AS Home" />
		<add key="DefaultLanguage" value="az-AZ" />
		<add key="ImageStorage" value="Images" />
		<add key="DefaultAdminUsername" value="admin" />
		<add key="DefaultAdminPassword" value="admin123" />
	</appSettings>
	<userSettings>
		<ASHome.Properties.Settings>
			<setting name="RememberUsername" serializeAs="String">
				<value />
			</setting>
		</ASHome.Properties.Settings>
	</userSettings>
